<div class="container">
  <h1>Latest Movie Reviews</h1>
  <div class="row">
<% dives.forEach((dive) => { %>
  <div class="col-md-3">
    <a href="/dives/<%= dive.id %>">
      <img class="thumbnail" src="<%= dive.image %>" alt="<%= dive.location %>">
      <h2> <%= dive.location %></h2>
      <p><% while(dive.stars--) { %> &star; <% } %></p>
    </a>
  </div>
<% }) %>
</div>
